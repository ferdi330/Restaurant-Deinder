@model IEnumerable<D_Einder_MVC.Models.MenuGerechten>

@{
    ViewData["Title"] = "Index";
}



<h1>Menu gerechten</h1>
<br /><p><a asp-action="Create" class="btn btn-success">Voeg Toe</a><p>
    <br />
<table id="customers">
    <thead>
        <tr>
           
            <th>
                Menu
            </th>
            <th>
                Gerechten van Menu
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.Menus.Naam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gerechten.Naam)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
